{
    "name": "tella86/final-project_ezems",
    "description": "Provides loader, dumper and an API for YAML content. Loader builds to equivalent data types in PHP 8.x",
    "type": "library",
    "keywords": [
        "yaml",
        "parser",
        "reader",
        "writer"
    ],
    "homepage": "https://github.com/Tella86/final-project_ezems.git",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Eng Ziroh",
            "email": "ezems.developers@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tella86/final-project_ezems\\Yaml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tella86/final-project_ezems\\Yaml\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1.2|8.2.12 ",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-SPL": "*",
        "lib-pcre": "*",
        "dallgoot/yaml": "^0.9.1"
    },

    "require-dev": {
        "ext-Reflection": "*",
        "phan/phan": "*",
        "phpmetrics/phpmetrics": "*",
        "phpunit/phpunit": "*"
    }
}
